@book{crookHowFrenchLearned2021,
  title = {How the {{French Learned}} to {{Vote}} : {{A History}} of {{Electoral Practice}} in {{France}}},
  shorttitle = {One {{Man}}, {{One Vote}}},
  author = {Crook, Malcolm},
  editor = {Crook, Malcolm},
  year = {2021},
  month = mar,
  publisher = {{Oxford University Press}},
  doi = {10.1093/oso/9780192894786.003.0002},
  urldate = {2023-04-04},
  abstract = {All adults are entitled to vote today, yet this principle was not established until relatively recently. Even in the case of men, who were invariably enfranchised before women, the recognition of universal male suffrage was far more problematic than is often realized, though much has been written about it. In fact, the belief that the franchise should be awarded on the basis of certain criteria, such as property ownership or tax payment, as opposed to constituting a right of citizenship, was widely accepted during the nineteenth century, in France as elsewhere. Universal manhood suffrage was proclaimed in the French Constitution of 1793, but not implemented, and its later foundation in 1848 was unanticipated. There were also periods of reversal as well as progress, with moments of disenfranchisement, notably under the restored monarchy after 1814, but also during the First and Second Republics. Even in the early 1870s, the principle of one man, one vote was still being challenged by conservatives and its consolidation under the Third Republic was by no means preordained; it had taken nearly a century to achieve.},
  isbn = {978-0-19-289478-6},
  keywords = {France,Franchise},
  file = {/Users/arturasro/Zotero/storage/RYZFS6XX/Crook - 2021 - One Man, One Vote The Long March towards Universa.pdf;/Users/arturasro/Zotero/storage/3T6ISJK5/339356914.html}
