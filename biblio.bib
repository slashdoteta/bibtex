@article{abou-chadiPoliticalInstitutionsDistributional2015,
  title = {Political {{Institutions}} and the {{Distributional Consequences}} of {{Suffrage Extension}}},
  author = {{Abou-Chadi}, Tarik and Orlowski, Matthias},
  year = {2015},
  journal = {Political Studies},
  volume = {63},
  number = {S1},
  pages = {55--72},
  issn = {1467-9248},
  doi = {10.1111/1467-9248.12193},
  urldate = {2023-02-08},
  abstract = {In this article, it is demonstrated that suffrage extension positively affects public goods provision and redistribution. This effect, however, depends on the political-institutional context within countries {\textendash} namely the electoral system and the existence of veto points. Since they incentivise more targeted spending, majoritarian systems decrease the positive effect that suffrage extension has on public goods provision. The relocation of the median voter through suffrage extension does not lead to more redistribution where second chambers with veto rights reduce the power of the working class to induce policy change. Empirical support is provided for this argument by analysing the conditional effects of suffrage extension through difference-in-difference estimation utilising data on government spending and inheritance taxation in eleven Western European countries between 1880 and 1938. These findings emphasise the importance of taking into account political institutions when assessing the determinants and consequences of democratisation.},
  langid = {english},
  keywords = {democratisation,electoral systems,government spending,suffrage extension,veto points},
  file = {/Users/arturasro/Zotero/storage/2EGVNWIS/Abou-Chadi and Orlowski - 2015 - Political Institutions and the Distributional Cons.pdf}
}

