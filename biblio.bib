@article{crookFirstEuropeanElections2014,
  title = {The {{First European Elections}}? {{Voting}} and {{Imperial State-Building Under Napoleon}}, 1802{\textendash}1813},
  shorttitle = {The {{First European Elections}}?},
  author = {Crook, Malcolm and Dunne, John},
