@article{carpenterSuffragePetitioningFormative2018,
  title = {Suffrage {{Petitioning}} as {{Formative Practice}}: {{American Women Presage}} and {{Prepare}} for the {{Vote}}, 1840{\textendash}1940},
  shorttitle = {Suffrage {{Petitioning}} as {{Formative Practice}}},
  author = {Carpenter, Daniel and Popp, Zachary and Resch, Tobias and Schneer, Benjamin and Topich, Nicole},
  year = {2018},
  month = apr,
  journal = {Studies in American Political Development},
  volume = {32},
  number = {1},
  pages = {24--48},
  publisher = {{Cambridge University Press}},
  issn = {0898-588X, 1469-8692},
  doi = {10.1017/S0898588X18000032},
  urldate = {2024-01-02},
  abstract = {The American woman suffrage movement remade the U.S. Constitution and effected the broadest expansion of voting eligibility in the nation's history. Yet it did more than change laws and citizenship. It also plausibly shaped participatory patterns before and after the winning of voting rights for women. Drawing upon the idea of formative practice and reporting on a range of historical materials{\textemdash}including an original data set of 2,157 petitions sent to the U.S. Congress from 1874 to 1920 concerning women's voting rights{\textemdash}we focus on woman suffrage petitioning as both presaging the practice of voting and, in a sense, preparing tens of thousands of women for that activity. Our analyses reveal that, before 1920, suffrage petitioning activity was heightened in general and midterm election years (especially among Republican-leaning constituencies), suffrage petitioning both enabled and reflected organization in critical western states, and that post-suffrage women's turnout was immediately and significantly higher in states with greater pre-suffrage petitioning (controlling for a range of political, organizational, and demographic variables). In its claims, symbolism, habits, and temporality, suffrage petitioning differed from other petitioning in American political development and marked a formative practice for women on their way to voting.},
  langid = {english},
  file = {/Users/arturasro/Zotero/storage/MX4JXUUD/Carpenter et al. - 2018 - Suffrage Petitioning as Formative Practice Americ.pdf}
}

