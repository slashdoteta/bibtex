@article{himmelfarbPoliticsDemocracyEnglish1966,
  title = {The {{Politics}} of {{Democracy}}: The {{English Reform Act}} of 1867},
  shorttitle = {The {{Politics}} of {{Democracy}}},
  author = {Himmelfarb, Gertrude},
